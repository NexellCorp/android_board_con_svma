/dev/block/mmcblk[0-9]*                                             u:object_r:nx_block_device:s0
/dev/block/mmcblk0p[0-9]*                                           u:object_r:nx_block_device:s0
/dev/ttyAMA[0-9]*                                                   u:object_r:console_device:s0
